  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.32.0",
    "@tieens-org/eslint-config": "workspace:*",
    "@tieens-org/ts-config": "workspace:*",
    "@types/lodash": "^4.17.0",
    "@types/nprogress": "^0.2.3",
    "@types/react": "18.2.73",
    "eslint": "^8.57.0",
